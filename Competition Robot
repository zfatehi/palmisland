task main()
{
  while(1 == 1)
	{

		motor[leftMotor] = vexRT[Ch3];
		motor[rightMotor] = vexRT[Ch2];

		if(vexRT[Btn6U] == 1)
		{
			motor[armMotor] = 40;
		}
		else if(vexRT[Btn6D] == 1)
		{
			motor[armMotor] = -40;
		}
		else
		{
			motor[armMotor] = 0;
		}

		if(vexRT[Btn5U] == 1)
		{
			motor[clawMotor] = 40;
		}
		else if(vexRT[Btn5D] == 1)
		{
			motor[clawMotor] = -40;
		}
		else
		{
			motor[clawMotor] = 0;
		}
	}


}
